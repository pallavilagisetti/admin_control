              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"token\": \"{{authToken}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/verify",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "verify"]
            }
          }
        },
