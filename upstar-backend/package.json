{
  "name": "upstar-backend",
  "version": "1.0.0",
  "description": "Upstar Backend API - Resume processing and job matching platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:prod": "NODE_ENV=production node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'No build step required for Node.js'",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "test:backend": "node test-backend.js",
    "test:db:remote": "node test-remote-db.js",
    "test:routes": "node test-routes.js",
    "test:routes:detailed": "node test-routes-detailed.js",
    "test:routes:all": "node run-route-tests.js",
    "test:all": "node run-tests.js",
    "setup:db": "node setup-database.js",
    "migrate": "node database/migrate.js up",
    "migrate:status": "node database/migrate.js status",
    "migrate:rollback": "node database/migrate.js rollback",
    "migrate:prod": "NODE_ENV=production node database/migrate.js up",
    "seed": "node database/seed.js run",
    "seed:status": "node database/seed.js status",
    "seed:rollback": "node database/seed.js rollback",
    "seed:reset": "node database/seed.js reset",
    "db:init": "npm run migrate && npm run seed",
    "db:init:prod": "npm run migrate:prod",
    "db:reset": "npm run seed:reset -- --confirm && npm run db:init",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write src/**/*.js",
    "docker:build": "docker build -f Dockerfile.production -t upstar-backend .",
    "docker:run": "docker run -p 5000:5000 --env-file env.local upstar-backend",
    "docker:compose:up": "docker-compose -f docker-compose.production.yml up -d",
    "docker:compose:down": "docker-compose -f docker-compose.production.yml down",
    "deploy:setup": "npm run migrate:prod && npm run start:prod",
    "health:check": "curl -f http://localhost:5000/api/health || exit 1"
  },
  "keywords": [
    "resume",
    "job-matching",
    "ai",
    "analytics",
    "api"
  ],
  "author": "Upstar Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1490.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.3.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lodash": "^4.17.21",
    "mammoth": "^1.6.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "redis": "^4.7.1",
    "sharp": "^0.32.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
